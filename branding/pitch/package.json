{
  "name": "lexoral-pitch-tool",
  "version": "1.0.0",
  "author": "Steven Waterman",
  "license": "GPL-3.0-only",
  "scripts": {
    "build-prod": "rollup -c --environment BRANCH:prod",
    "build-prod-demo": "rollup -c --environment BRANCH:prod --environment DEMO:true",
    "build-stage": "rollup -c --environment BRANCH:stage",
    "build-stage-demo": "rollup -c --environment BRANCH:stage --environment DEMO:true",
    "dev": "rollup -c -w --environment BRANCH:stage",
    "dev-demo": "rollup -c -w --environment BRANCH:stage --environment DEMO:true",
    "validate": "svelte-check",
    "start": "sirv public --no-clear"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.2.3",
    "@tsconfig/svelte": "^1.0.13",
    "@types/node": "^16.4.1",
    "rollup": "^2.53.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.2.5",
    "svelte-preprocess": "^4.7.4",
    "tslib": "^2.0.0",
    "typescript": "^4.4.0"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0"
  }
}
